{
	"name": "grandmasterx/interkassa",
	"description": "Extension for integration Interkassa in yii2 project",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"extension",
		"interkassa"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "grandmasterx",
			"email": "zgrandmasterz@gmail.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"fxp/composer-asset-plugin": "dev-master",
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-httpclient": "~2.0.0"
	},
	"prefer-stable": true,
    "minimum-stability": "dev",
	"require-dev": {
		"phpunit/phpunit": "~4.5,>=4.5.1"
	},
	"autoload": {
		"psr-4": {
			"grandmasterx\\interkassa\\": ""
		}
	},
	"extra": {
		"asset-installer-paths": {
			"bower-asset-library": "vendor/bower"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
